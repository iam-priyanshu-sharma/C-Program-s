/*Program to enter the obtained marks,percentage of a student and print the C.G.P.A.*/
#include<stdio.h>
#include<conio.h>
void main()
     {
     int Marks;
     float Percentage;
     clrscr();
     printf("\nEnter total marks of a student:");
     scanf("%d",&Marks);
     Percentage=Marks*0.2;
     printf("Percentage=%.2f\n",Percentage);
     if(Percentage>=90)
	printf("C.G.P.A.=10");
     else if(Percentage>=80)
	    printf("C.G.P.A.=9");
	  else if(Percentage>=70)
		  printf("C.G.P.A.=8");
	       else if(Percentage>=60)
		       printf("C.G.P.A.=7");
		    else if(Percentage>=50)
			   printf("C.G.P.A.=6");
			 else if(Percentage>=40)
				printf("C.G.P.A.=5");
			      else if(Percentage>=30)
				     printf("C.G.P.A.=4");
				   else
				       printf("Fail");
     getch();
     }